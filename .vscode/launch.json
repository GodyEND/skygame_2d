    "version": "0.2.0",
    "configurations": [
        {
            "name": "skygame_2d",
            "request": "launch",
            "type": "dart",
            "deviceId": "chrome",
            "args": [
                "--web-renderer",
                "canvaskit"
            ]
        },
        {
            "name": "skygame_2d (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "skygame_2d (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        },
        {
            "name": "Dart: Run all Tests",
            "request": "launch",
            "type": "dart",
            "program": "./test/"
        }
    ]
}